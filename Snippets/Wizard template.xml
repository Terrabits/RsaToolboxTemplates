<wizard version="1" kind="project"
    class="qmakeproject" firstpage="10"
    id="A.TEMPLATE_NAME" category="B.Rohde-Schwarz">
    <icon>RS.png</icon>
    <description>_DESCRIPTION_</description>
    <displayname>_NAME_</displayname>;
    <displaycategory>Rohde &amp; Schwarz America</displaycategory>
    <files>
      
      <!-- RsaToolbox Files Snippet -->


      <!-- Project source -->
      <file source="Keys/Debug keys go here" target="Keys/Debug keys go here" openeditor="false" binary="true" />
      <file source="DEBUG_LOGFILE.txt" openeditor="false" />
      <file source="Settings.h" openeditor="false" />
      <file source="main.cpp" openeditor="false" />
      <file source="MeasureThread.h" openeditor="false" />
      <file source="MeasureThread.cpp" openeditor="false" />
      <file source="mainwindow.ui" openeditor="true" />
      <file source="mainwindow.h" openeditor="false" />
      <file source="mainwindow.cpp" openeditor="false" />
      <file source="project.pro" target="%ProjectName%.pro" openproject="true" />
  </files>
  <!-- Create a 2nd wizard page with parameters -->
  <fieldpagetitle>RsaToolbox Settings</fieldpagetitle>
  <fields>
    <field mandatory="true" name="APPLICATION_NAME">
        <fieldcontrol class="QLineEdit" validator='^[^"]+$'
          defaulttext="My App" />
          <fielddescription>Application Name:</fielddescription>
      </field>
      <field mandatory="true" name="APP_FOLDER">
        <fieldcontrol class="QLineEdit" validator='^[^"]+$'
          defaulttext="My App" />
          <fielddescription>Installation folder name:</fielddescription>
      </field>
      <field mandatory="true" name="LOG_FILENAME">
        <fieldcontrol class="QLineEdit" validator='^[^"]+$'
          defaulttext="My App Log.txt" />
          <fielddescription>Log filename:</fielddescription>
      </field>
  </fields>
</wizard>